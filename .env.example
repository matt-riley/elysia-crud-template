# Copy to .env (Compose + local dev both read KEY=value format)
DB_NAME=quotes
DB_USER=quoter
DB_PASS=test
# For Docker Compose, keep this as "db" (the service name)
DB_HOST=db
# Postgres port (defaults to 5432)
DB_PORT=5432

# Logger level (pino): fatal, error, warn, info, debug, trace
LOG_LEVEL=info
